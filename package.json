{

  "name": "@tillpos/rn-receipt-printer-utils",
  "version": "0.0.14",
  "description": "Fork of react-native-printer. A React Native Library to support USB/BLE/Net printer",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "author": "Till",
  "homepage":"https://github.com/hitz-group/rn-receipt-printer-utils.git",
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/hitz-group/rn-receipt-printer-utils.git"
  },
  "files": [
    "lib",
    "android",
    "ios",
    "tillpos-rn-receipt-printer-utils.podspec"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hitz-group/rn-receipt-printer-utils/issues"
  },
  "keywords": [
    "react-native",
    "thermal",
    "printer",
    "bluetooth",
    "net",
    "printer",
    "rn",
    "android"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "pod-install": "^0.1.10",
    "react": "^16.13.1",
    "react-native": "^0.63.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "ts-loader": "^2.3.4",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "react-native-builder-bob": "^0.17.1",
    "@types/react-native": "0.62.13"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
